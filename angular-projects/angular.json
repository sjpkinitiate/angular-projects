{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "job-management-system": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/job-management-system",
      "sourceRoot": "projects/job-management-system/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/job-management-system",
            "index": "projects/job-management-system/src/index.html",
            "browser": "projects/job-management-system/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/job-management-system/tsconfig.app.json",
            "assets": [
              "projects/job-management-system/src/favicon.ico",
              "projects/job-management-system/src/assets"
            ],
            "styles": [
              "projects/job-management-system/src/styles.css",
              "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
            ],
            "scripts": [
              "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "job-management-system:build:production"
            },
            "development": {
              "buildTarget": "job-management-system:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "job-management-system:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/job-management-system/tsconfig.spec.json",
            "assets": [
              "projects/job-management-system/src/favicon.ico",
              "projects/job-management-system/src/assets"
            ],
            "styles": [
              "projects/job-management-system/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "myGIS": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/my-gis",
      "sourceRoot": "projects/my-gis/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/my-gis",
            "index": "projects/my-gis/src/index.html",
            "browser": "projects/my-gis/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/my-gis/tsconfig.app.json",
            "assets": [
              "projects/my-gis/src/favicon.ico",
              "projects/my-gis/src/assets"
            ],
            "styles": [
              "projects/my-gis/src/styles.css"
            ],
            "scripts": [],
            "server": "projects/my-gis/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/my-gis/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "myGIS:build:production"
            },
            "development": {
              "buildTarget": "myGIS:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "myGIS:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/my-gis/tsconfig.spec.json",
            "assets": [
              "projects/my-gis/src/favicon.ico",
              "projects/my-gis/src/assets"
            ],
            "styles": [
              "projects/my-gis/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
